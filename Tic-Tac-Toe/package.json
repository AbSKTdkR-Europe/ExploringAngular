  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n_1": "ngx-translate-extract --input ./src/app/registration --output ./src/assets/i18n/{en,es,de}.json --clean --sort --format namespaced-json",
    "i18n_2": "ngx-translate-extract --input ./src/app/highscore --output ./src/assets/i18n/highscore/{en,es,de}.json --clean --sort --format namespaced-json",
    "extract": "npm run i18n_1 && npm run i18n_2"
  },
  "private": true,
