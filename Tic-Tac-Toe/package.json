    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "protractor": "~5.1.2",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "~2.4.1"
  }
